{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "crons": [
    {
      "path": "/api/crons/bucket-cleanup",
      "schedule": "50 * * * *"
    }
  ],
  "functions": {
    "api/render-html": {
      "memory": 3008,
      "maxDuration": 300,
      "includeFiles": [
        "node_modules/@sparticuz/chromium/bin/**",
        "node_modules/@sparticuz/chromium/lib/**"
      ]
    }
  }
}
