{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": [
    "ANALYZE",
    "AWS_LAMBDA_JS_RUNTIME",
    "NEXT_PUBLIC_NODE_ENV",
    "NEXT_PUBLIC_WS_SERVER",
    "SHARED_KEY",
    "ICE_SERVER_CONFIG",
    "HEADLESS_RENDER_ENABLED",
    "HEADLESS_RENDER_URL",
    "TURSO_URL",
    "TURSO_TOKEN",
    "NODE_ENV",
    "NEXTAUTH_SECRET",
    "NEXTAUTH_URL",
    "VERCEL_URL",
    "UNSPLASH_APP_ID",
    "UNSPLASH_ACCESS_KEY",
    "UNSPLASH_SECRET_KEY",
    "NEXT_PUBLIC_UNSPLASH_APP_NAME",
    "SUPABASE_S3_ACCESS_KEY_ID",
    "SUPABASE_S3_SECRET_ACCESS_KEY",
    "SUPABASE_S3_ENDPOINT",
    "SUPABASE_S3_REGION",
    "SUPABASE_S3_BUCKET",
    "SUPABASE_KEY",
    "SUPABASE_URL",
    "BLOB_READ_WRITE_TOKEN",
    "VERCEL_BLOB_STORAGE_HOST",
    "GITHUB_CLIENT_ID",
    "GITHUB_CLIENT_SECRET",
    "NEXT_PUBLIC_FIRESTORE_API_KEY",
    "NEXT_PUBLIC_FIRESTORE_AUTH_DOMAIN",
    "NEXT_PUBLIC_FIRESTORE_PROJECT_ID",
    "NEXT_PUBLIC_FIRESTORE_STORAGE_BUCKET",
    "NEXT_PUBLIC_FIRESTORE_MESSAGING_SENDER_ID",
    "NEXT_PUBLIC_FIRESTORE_APP_ID",
    "CRON_SECRET",
    "SKIP_ENV_VALIDATION",
    "REACT_SCAN_ENABLED",
    "MEDIA_DOWNLOADER_URL",
    "MEDIA_DOWNLOADER_PORT"
  ],
  "globalDependencies": [
    ".env",
    ".env.development",
    ".env.production",
    ".env.test"
  ],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "dist/**",
        "packages/lexical/packages/*/dist/**"
      ]
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "inputs": [".env.development"]
    },
    "start": {
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "db:studio": {
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "db:generate": {},
    "db:pull": {},
    "test:ts": {
      "dependsOn": ["^test"],
      "outputs": []
    }
  }
}
