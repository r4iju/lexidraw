{
  "name": "@packages/lib",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "bun x tsgo --watch --project tsconfig.json",
    "build": "bun x tsgo --project tsconfig.json",
    "lint": "bun x @biomejs/biome check --write .",
    "test:ts": "bun x tsgo --noEmit --project tsconfig.json",
    "clean": "rm tsconfig.tsbuildinfo && rm -rf ./dist && rm -rf .turbo && rm -rf node_modules"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./*": [
      "./dist/*.js",
      "./dist/*/index.js"
    ]
  },
  "dependencies": {
    "@packages/env": "workspace:*",
    "@vercel/blob": "^1.0.2",
    "zod": "^4.1.12"
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*",
        "src/*/index"
      ]
    }
  },
  "publishConfig": {
    "typesVersions": {
      "*": {
        "*": [
          "dist/*.d.ts",
          "dist/*/index.d.ts"
        ]
      }
    }
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/node": "^22.15.19",
    "@typescript/native-preview": "^7.0.0-dev.20251214.1"
  }
}
